@model IEnumerable<DemoMvcApplication.Models.PatientViewModel>

@{
    ViewBag.Title = "Index";
}

@Html.ActionLink("Logout","Logout","User")

<h2>Patients</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
    </tr>
}

</table>
